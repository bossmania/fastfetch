{
  //Provides IDE intelligence
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    //point to where the logo lives at, and make it blue
    "type": "file",
    "source": "$HOME/.config/fastfetch/logo.txt",
    "color": { "1": "#1794d2", "2":"#1794d2"}
  },
  "display": {
    "color": {
      //change the color of the text
      "keys": "cyan",
      "title": "#07e807",
      "output": "white"
    },
    // "key": {
    //   //show the icon and string for the key
    //   "type": "both"
    // },
    //make it show a color percentage that change colors
    "percent": {
      "type": 9,
      "color": {
        "green": "green",
        "yellow": "light_yellow",
        "red": "light_red"
      }
    }
  },
  "modules": [
    //title
    "title",
    "separator",

    //distro info
    {
    "type": "os",
    "key": " DISTRO",
    "keyColor": "#f9bb11"
    },
    {
    "type": "kernel",
    "key": "├─ Kernal",
    "keyColor": "#ba8f14"
    },
    {
    "type": "packages",
    "key": "├─󰏖 Packages",
    "keyColor": "#ba8f14"
    },
    {
    "type": "shell",
    "key": "├─ Shell",
    "keyColor": "#ba8f14"
    },
    {
      "type": "uptime",
      "key": "└─󰅐 Uptime",
      "keyColor": "#ba8f14"
    },

    "break",

    //DE/WM info
    {
      "type": "wm",
      "key": " DE/WM",
      "keyColor": "#0096b3"
    },
    {
      "type": "wmtheme",
      "key": "├─󰉼 WM Theme",
      "keyColor": "blue"
    },
    {
      "type": "icons",
      "key": "├─󰀻 Icons",
      "keyColor": "blue"
    },
    {
      "type": "cursor",
      "key": "├─󰆿 Cursor",
      "keyColor": "blue"
    },
    {
      "type": "terminal",
      "key": "├─ Terminal",
      "keyColor": "blue"
    },
    {
     	"type": "terminalfont",
      "key": "├─ Terminal Font",
      "keyColor": "blue"
    },
    {
     	"type": "font",
      "key": "├─ Font",
      "keyColor": "blue"
    },
    {
     	"type": "locale",
      "key": "└─  Lang",
      "keyColor": "blue"
    },
    "break"

